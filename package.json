      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
